{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "title": {
    "text": "Unemployment rate ",
    "anchor": "start",
    "subtitle": "Source: JSB (WB) through ECO API",
    "fontSize": 14,
    "subtitleFontSize": 8,
    "color": "#000000",
    "subtitleColor": "#000000"
  },
  "height": 250,
  "width": "container",
  "autosize": {"type": "fit", "contains": "padding", "resize": false},
  "background": "transparent",
  "view": {"fill": "transparent", "strokeOpacity": 0},
  "data": {
    "url": "https://api.economicsobservatory.com/jpn/unem?vega",
    "format": {"type": "json"}
  },
  "mark": {"type": "bar", "color": "#0091ff", "strokeWidth": 2},
  "encoding": {
    "x": {
      "field": "date",
      "type": "temporal",
      "axis": {"title": "Years", "titleColor": "#122B39", "grid": false}
    },
    "y": {
      "field": "value",
      "type": "quantitative",
      "axis": {
        "title": "Unemployment rate ",
        "titleColor": "#122B39",
        "grid": false
      }
    }
  },
  "config": {}
}