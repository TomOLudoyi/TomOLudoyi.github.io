{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
      
    "data": {
      "url": "https://download.beta.ons.gov.uk/downloads/datasets/cpih01/editions/time-series/versions/11.csv"},
  
    "transform":[
      
      {
        "filter": {
          "field": "Aggregate", 
          "oneOf": ["01.1 Food", "03.1 Clothing", "10 Education", "07.1.1 New Cars"]
        }
      },
      {
        "filter": {
          "field": "Time",
          "range": [
            {"year": 1988},
            {"year": 2000}
          ]
        }
      }
      ],
   
    "width":300,
    
    "mark": "line",
    
    "encoding": {
      
      "x": {
        "field": "Time",
        "type": "temporal",
        "timeUnit":{
          "unit":"yearmonth"}
      },
      
      "y": {"field": "v4_0", "type": "quantitative", "title": null},
  
      "color":{"field":"Aggregate"}
    }
  }